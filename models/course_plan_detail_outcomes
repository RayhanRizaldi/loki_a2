const Sequelize = require('sequelize');
const sequelize = new Sequelize ("mysql://root@localhost:3306/loki")

const detail_outcomes = sequelize.define('course_plan_detail_outcomes', {
    id : {
        type: Sequelize.INTEGER,
        autoIncrement: true,
        primaryKey: true
    },

    course_plan_detail_id: {
        type: Sequelize.INTEGER,
        autoIncrement: true,
        foreignKey: true,
    },

    course_lo_id: {
        type: DataTypes.INTEGER,
        autoIncrement: true,
        foreignKey: true,
    },

    created_at: {
        type: DataTypes.DATE,
    },

    update_at: {
        type: DataTypes.DATE,
    }
}, 
{
    tableName: 'course_plan_detail_outcomes',
    timestamps: true  
});

module.exports = detail_outcomes;