<%- include('../partials/sidebar'); -%>

<div id="container">
  
  <h3><i class="fa-solid fa-list me-2"></i>List RPS</h3><hr>
                <div class="container">
                    <div class="row">
                        <div class="col-2"><h6>Tahun Akademik</h6></div>
                        <div class="col-2"><h6>: 2021/2022</h6></div>
                      </div>
                    <div class="row">
                      <div class="col-2"><h6>Semester</h6></div>
                      <div class="col-3"><h6>: Genap</h6></div>
                    </div><br>
                </div>
              
              
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
        <th class="text-center" scope="col">No.</th>
        <th class="text-center" scope="col">Nama Mata Kuliah</th>
        <th class="text-center" scope="col">Kode Mata Kuliah</th>
        <th class="text-center" scope="col">SKS</th>
        <th class="text-center" scope="col">Sem</th>
        <th class="text-center" scope="col">Jumlah Revisi</th>
        <th class="text-center" scope="col">Lihat</th>
        <th class="text-center" scope="col">Cetak</th>
        <th class="text-center" scope="col">Detail</th>
      </tr>
    </thead>
    <tbody>
      <% items.forEach((item,i) => { %>
      <tr>
        
        <td scope="row"><%= i+1 %></td>
        <td class="text-center"><%= item.name %></td>
        <td class="text-center"><%= item.code %></td>
        <td class="text-center"><%= item.credit %></td>
        <td class="text-center"><%= item.semester %></td>
        <td class="text-center"><%= item.rev %></td>
        <td class="text-center"><a href="" class="link-info"><i class='bx bx-sm bx-detail bx-border'></i></a></td>
        <td class="text-center"><a href="" class="link-primary"><i class='bx bx-sm bx-printer bx-border'></i></a></i></td>
        <td class="text-center">
          <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
            Details
            </a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
              <a class="dropdown-item" href="/admin/coursesPlan/<%= item.id%>">Kelola Dosen</a>
              <a class="dropdown-item" href="#">Peta CPMK ke CPL</a>
              <a class="dropdown-item" href="#">Persentase RPS</a>
            </div>
        </td>
       
      </tr>
      <% }) %>
      </tbody>
  </table>
    
    
</div>

<%- include('../partials/footer'); -%>
