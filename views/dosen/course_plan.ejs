<%- include('../partials/sidebar'); -%>
     
        <div id="container">
          <div class="content">
            <div class="row detail-rps">
                <% item.forEach((item) => { %>
                <nav class="navbar navbar-expand-lg navbar-light">
                    <div>
                        <h1 class="navbar-brand"><%= item.name %></h1>
                        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
                          <span class="navbar-toggler-icon"></span>
                        </button>
                    </div>
                
                    <div class="collapse navbar-collapse justify-content-end" id="navbarText">
                      <ul class="navbar-nav mr-auto">
                        <li class="nav-item active">
                          <a class="nav-link" href="/dosen/coursesPlan/<%= item.id %>/CPMK">CPMK  /</a>
                        </li>
                        <li class="nav-item">
                          <a class="nav-link" href="/dosen/coursesPlan/<%= item.id %>/referensi">Referensi  /</a>
                        </li>
                        <li class="nav-item">
                          <a class="nav-link" href="/dosen/coursesPlan/<%= item.id %>/penilaian">Penilaian  /</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="/dosen/coursesPlan/<%= item.id %>/pertemuan">Pertemuan</a>
                          </li>
                      </ul>
                    </div>
                  </nav>
                  <% }) %>
            </div>
            
            <div class="tampil-rps">
                
                <table class="rps-tabel">
                     <thead>
                        
                        <tr>
                            
                            <th rowspan="4", align="center"><img src="/images/logounand.png" alt="logounand"  /></th>
                            <th colspan="7">RENCANA PEMBELAJARAN SEMESTER (RPS) </th> 
                        </tr>
                        <tr>
                            <th colspan="7">PROGRAM STUDI: Sistem Informasi </th>
                        </tr>
                        <tr>
                            <th colspan="7">FAKULTAS/PPs: Teknologi Informasi</th>
                        </tr>
                        <tr>
                            <th colspan="7">UNIVERSITAS ANDALAS </th>
                        </tr>
                       

                        

                    </thead>
                    <tbody>
                        <tr>
                            <td colspan="2">MATA KULIAH </td>
                            <td>KODE </td>
                            <td colspan="2">Rumpun/Kelompok MK </td>
                            <td>BOBOT(sks) </td>
                            <td>Semester </td>
                            <td align="center">Tgl Penyusunan </td>
                        </tr>
                        <% item.forEach((item) => { %>

                        <tr>
                            <td colspan="2"><%= item.name %></td>
                            <td><%= item.code %></td>
                            <td colspan="2"> </td>
                            <td ><%= item.credit %></td>
                            <td ><%= item.semester %></td>
                            <td > </td>
                        </tr>
                        <% }) %>
                        <tr>
                            <td rowspan="2" colspan="2">OTORISASI </td>
                            <td colspan="2">Dosen Pengembang RPS </td>
                            <td colspan="2">Koordinator Rumpun MK </td>
                            <td colspan="2">KA Program Studi </td> 
                        </tr>
                        <tr>
                            <td colspan="2"></td>
                            <td colspan="2"> </td>
                            <td colspan="2"> </td> 
                        </tr>

                        <tr>
                            <td rowspan="8">Capaian Pembelajaran (CP) </td>
                            <td colspan="7" class="grey">CP Program Studi </td> 
                        </tr>
                        <tr>
                            <td>CP-1 </td> 
                            <td colspan="6">Mengidentifikasi </td> 
                        </tr>
                        <tr>
                            <td>CP-2 </td> 
                            <td colspan="6">Mengintegrasikan </td> 
                        </tr>
                        <tr>
                            <td>CP-3 </td> 
                            <td colspan="6">Menerapkan konsep </td> 
                        </tr>
                        <tr>  
                            <td colspan="7" class="grey">CP Mata Kuliah </td> 
                        </tr>
                       

                            <% item.forEach((data) => { 
                                data.course_los.forEach((res) => { %>
                        <tr>
                            <td><%= res.code %> </td> 
                            <td colspan="6"><%= res.name  %> </td> 
                        </tr>
                        <% })}) %>
                        <% item.forEach((item) => { %>
                        <tr>
                            <td >Deskripsi Singkat Mata Kuliah </td>
                            <td colspan="7"><%= item.description %></td> 
                        </tr>
  
                        <tr>
                            <td >Materi Pembelajaran/Pokok Bahasan </td>
                            <td colspan="7"><%= item.material %></td> 
                        </tr>

                        <tr>
                            <td colspan="10">Pustaka </td>
                        </tr>
                        <% }) %>
                        <% item.forEach((data) => { 
                            data.course_plan_references.forEach((res) => { var no = 0  %>
                        <tr>
                            <td rowspan="1" class="border-bottom-0"></td>
                            <td><%= res.id  %> </td> 
                            <td colspan="6"><%= res.author  %>, <%= res.year  %>, "<%= res.title  %>", <%= res.publisher  %></td> 
                        </tr>
                        <% })}) %>



                        <tr>
                            <td rowspan="8">Media Pembelajaran </td>
                            <td colspan="3" class="grey">Perangkat Lunak: </td> 
                            <td colspan="4" class="grey">Perangkat Keras: </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">Power point </td> 
                            <td colspan="4">Projector </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">Code Editor </td> 
                            <td colspan="4">Laptop </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">Browser </td> 
                            <td colspan="4"> </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">NodeJs </td> 
                            <td colspan="4"> </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">Rest Clien (Postman) </td> 
                            <td colspan="4"> </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">Database Server (MYSQL) </td> 
                            <td colspan="4"> </td> 
                        </tr>
                        <tr> 
                            <td colspan="3">Version Control (GIT) </td> 
                            <td colspan="4"> </td> 
                        </tr>

                        <tr>
                            <td>Team Teaching </td>
                            <td colspan="7">- </td> 
                        </tr>
                        <tr>
                            <td colspan="10">Assessment </td>
                        </tr>
                        <% item.forEach((data) => { 
                            data.course_plan_assessments.forEach((res) => { var no = 0  %>
                        <tr>
                            <td rowspan="1" class="border-hidden"></td>
                            <td colspan="7" rowspan=""><%= res.name  %>    : <%= res.percentage  %></td> 
                            <% })}) %>

                        </tr>

                        <tr>
                            <td rowspan="3">Mata Kuliah Syarat </td>
                        </tr>
 
                    </tbody>
                </table>
            </div>

          </div>
        </div>
      </div>

    <%- include('../partials/footer'); -%>